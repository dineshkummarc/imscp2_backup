<config>
	<nav>
		<users>
			<label>Manage Users</label>
			<module>core</module>
			<controller>user</controller>
			<route>core_user_list</route>
			<class>manage_users</class>
			<order>200</order>
			<pages>
				<list>
					<label>Overview</label>
					<module>core</module>
					<controller>user</controller>
					<action>list</action>
					<route>core_user_list</route>
				</list>
				<add>
					<label>Add user</label>
					<module>core</module>
					<controller>user</controller>
					<action>add</action>
					<route>core_user_add</route>
				</add>
				<changepassword>
					<label>Change password</label>
					<module>core</module>
					<controller>user</controller>
					<action>changepassword</action>
					<route>core_user_changepassword</route>
				</changepassword>
			</pages>
		</users>
	</nav>
</config>
